    "redux-devtools-inspector": "^0.10.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-slider-monitor": "^1.0.2",
    "reporter": "^0.1.0",
    "sinon": "^2.0.0-pre",
    "webpack": "^2.2.0",
